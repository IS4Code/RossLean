<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>12.0</LangVersion>
    <SuppressDependenciesWhenPacking>true</SuppressDependenciesWhenPacking>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Alex Kalfakakos</Authors>
    <Product>GenericsAnalyzer.Core</Product>
    <PackageId>GenericsAnalyzer.Core</PackageId>
    <Copyright>© 2021, Alex Kalfakakos</Copyright>
    <Description>The core components for using the generics analyzer.</Description>
    <RepositoryUrl>https://github.com/RossLean/RossLean/tree/master/RossLean.GenericsAnalyzer</RepositoryUrl>
    <PackageTags>roslyn, analyzer, generics, constraints</PackageTags>
    <Version>1.3.4-rc1</Version>
    <PackageVersion>1.3.4-rc1</PackageVersion>
    <RootNamespace>RossLean.GenericsAnalyzer.Core</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Garyon" Version="0.3.3" />
    <PackageReference Include="Microsoft.CodeAnalysis" Version="4.8.0" />
    <PackageReference Include="RoseLynn" Version="0.6.4.1" />
  </ItemGroup>

</Project>
